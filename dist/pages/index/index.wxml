            <navigator class="bar-box" wx:key="index" wx:for="{{barSet}}" wx:for-item="nav" wx:for-index="index">
                <cover-image class="bar-icon" src="{{nav.icon}}"></cover-image>
                <text class="bar-text">{{nav.name}}</text>
            </navigator>
        </view>
        <view class="clear-line"></view>
        <view class="home-box">
            <ctitle title="321" __triggerObserer="{{ _triggerObserer }}" label="çƒ­" tab="{{true}}" bindontab="tabHandle" __fn_onTab="{{true}}" data-e-ontab-so="this"></ctitle>
            <view class="book-hot">
                <block-box-a data="{{Data.hot}}" __triggerObserer="{{ _triggerObserer }}"></block-box-a>
            </view>
        </view>
    </view>
</block>