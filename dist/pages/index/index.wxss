
.swiper-box {
  height: 360rpx;
}
.nav-bar {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
}
.nav-bar .bar-box {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  text-align: center;
}
.nav-bar .bar-icon {
  width: 60rpx;
  height: 60rpx;
  margin: auto;
}
.nav-bar .bar-text {
  font-size: 30rpx;
}
.clear-line {
  height: 20rpx;
  background: #f5f5f5;
  clear: both;
  width: 100%;
}
