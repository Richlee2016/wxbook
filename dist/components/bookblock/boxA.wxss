
.block-box-a .box {
  width: 250rpx;
  float: left;
  text-align: center;
  margin-top: 20rpx;
}
.block-box-a .img {
  width: 230rpx;
  height: 330rpx;
}
.block-box-a .txt {
  padding: 0 20rpx;
  font-size: 26rpx;
  max-height: 30rpx;
  line-height: 30rpx;
  display: block;
  /*内联对象需加*/
  word-break: keep-all;
  /* 不换行 */
  white-space: nowrap;
  /* 不换行 */
  overflow: hidden;
  /* 内容超出宽度时隐藏超出部分的内容 */
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
}
