
.c-search {
  margin: 20rpx 75rpx;
  width: 600rpx;
  height: 72rpx;
  border: 2rpx solid #f5f5f5;
  overflow: hidden;
}
.c-search .s-icon {
  width: 72rpx;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  border-right: 1rpx solid #f5f5f5;
  float: left;
}
.c-search .s-icon .icon {
  height: 56rpx;
  padding-top: 12rpx;
}
.c-search .s-inp {
  float: left;
  width: 510rpx;
  height: 72rpx;
  padding-left: 18rpx;
}
.c-book-title {
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid #f0f0f0;
  padding-left: 40rpx;
  border-left: 12rpx solid #ffab18;
  overflow: hidden;
}
.c-book-title .title {
  float: left;
  font-size: 32rpx;
}
.c-book-title .label {
  display: inline-block;
  width: 52rpx;
  height: 32rpx;
  line-height: 32rpx;
  color: white;
  text-align: center;
  background: #53ac7d;
  font-size: 24rpx;
  margin-left: 16rpx;
}
.c-book-title .active {
  color: #528ae8;
}
.c-book-title .tab {
  float: right;
  font-size: 26rpx;
  margin-right: 20rpx;
}
.c-book-title .tab .tab-t {
  padding: 20rpx 20rpx;
}
